<%- include("../partials/header") %>

<h1><%= campground.name %> </h1>

<img src="<%= campground.image %> " alt="">

<p><%= campground.description %> </p>

<% campground.comments.forEach(comment => { %>
    <p>
        <strong>
            <%= comment.author %> - <%= comment.text %>
        </strong>
    </p>
<% }); %>

<%- include("../partials/footer") %>